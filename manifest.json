{
  "manifest_version": 3,
  "name": "applytweet",
  "description": "Replaces the new \"Post\" button with the original \"Tweet\" button",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}
